// Redirection vers la version anglaise (sans [lang])
export const prerender = true;

export async function getStaticPaths() {
  const langs = ['en', 'fr', 'es', 'ru', 'cn'];
  return langs.map((lang) => ({
    params: { lang },
  }));
}

